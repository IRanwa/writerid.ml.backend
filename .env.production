# WriterID Python Backend - Production Environment Variables

# Backend API URL - Production backend API endpoint
BACKEND_API_URL=https://your-production-backend.com/api

# Azure Storage Connection String - Production Azure storage credentials
AZURE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your_prod_account_name;AccountKey=your_prod_account_key;EndpointSuffix=core.windows.net

# Production specific settings
DEBUG=False
LOG_LEVEL=INFO
ENVIRONMENT=production 